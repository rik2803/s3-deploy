{
  "parser": "babel-eslint",
  "rules": {
    "strict": 0,
    "indent": [
        2,
        2
    ],
    "quotes": [
        2,
        "single"
    ],
    "linebreak-style": [
        2,
        "unix"
    ],
    "semi": [
        2,
        "always"
    ],
    "no-underscore-dangle": [0],
    "no-console": [0]
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "ecmaFeatures": {
    "jsx": true
  },
  "globals": {
    "process": false
  }
}
